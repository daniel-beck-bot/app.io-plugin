<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
  <f:invisibleEntry>
    <f:textbox field="id"/>
  </f:invisibleEntry>
  <f:entry title="${%Amazon AWS Access Key}" field="s3AccessKey">
    <f:textbox/>
  </f:entry>
  <f:entry title="${%Amazon AWS Secret Key}" field="s3SecretKey">
    <f:password/>
  </f:entry>
  <f:entry title="${%Amazon AWS S3 Bucket}" field="s3Bucket">
    <f:textbox/>
  </f:entry>  
  <f:entry title="${%API key}" field="apiKey">
    <f:password/>
  </f:entry>
</j:jelly>